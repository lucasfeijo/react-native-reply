{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2017",
      "es7",
      "es6",
      "dom"
    ],
    "module": "commonjs",
    "declaration": true,
    "outDir": "./lib",
    "strict": false,
    "jsx": "react-native",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "lib"
  ]
}